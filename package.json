{
  "name": "nodejs-typescript-sequelize-app",
  "version": "1.0.0",
  "description": "Node.js TypeScript application with Sequelize and Express",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsnd --respawn -r tsconfig-paths/register bin/www.ts start",
    "watch": "tsc -w",
    "dev:debug": "nodemon --inspect src/app.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "sequelize",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bunyan": "^1.8.15",
    "commander": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mysql2": "^3.4.0",
    "sequelize": "^6.32.1",
    "tsconfig-paths": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.18.38",
    "@types/validator": "^13.7.17",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
